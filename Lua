sparkle.BackgroundTransparency = 1
sparkle.Image = "rbxassetid://3926305904"
sparkle.ImageTransparency = 0.6
sparkle.Visible = false
sparkle.Parent = mainFrame

local sparkleTween = TweenService:Create(
	sparkle,
	TweenInfo.new(1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, -1),
	{Rotation = 360}
)

-- Effects
local function pop(obj)
	local base = obj.Size
	local t1 = TweenService:Create(obj, TweenInfo.new(0.12, Enum.EasingStyle.Back), {
		Size = base + UDim2.new(0, 6, 0, 6)
	})
	local t2 = TweenService:Create(obj, TweenInfo.new(0.12, Enum.EasingStyle.Back), {
		Size = base
	})
	t1:Play()
	t1.Completed:Wait()
	t2:Play()
end

local function fadeIn(frame)
	frame.BackgroundTransparency = 0.25
	for _,v in ipairs(frame:GetDescendants()) do
		if v:IsA("TextLabel") or v:IsA("TextButton") then
			v.TextTransparency = 0.25
		end
	end
	task.delay(0.15, function()
		frame.BackgroundTransparency = 0
		for _,
